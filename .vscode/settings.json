		"chosungIncludes",
		"Digimon",
		"dont",
		"Gacha",
		"Greuta",
		"hugeicons",
		"iconify",
		"inven",
		"mingcute",
		"nocompatible",
		"ooui",
		"signin",
		"weui"
	]
}
